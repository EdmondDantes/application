{
    "name": "ifcastle/application",
    "description": "A set of interfaces for the \"Application\" pattern.",
    "type": "library",
    "license": "MIT",
    "keywords": ["application", "interfaces", "patterns"],
    "autoload": {
        "psr-4": {
            "IfCastle\\Application\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IfCastle\\Application\\": "./tests"
        }
    },
    "require": {
        "php": "^8.4",
        "ifcastle/exceptions": "^5.0",
        "ifcastle/di": "^0.8.0",
        "ifcastle/design-patterns": "^0.4",
        "ifcastle/os-utilities": "^0.5.0",
        "psr/log": "^3.0",
        "ifcastle/service-manager": "^0.7.0",
        "ifcastle/async-contracts": "^0.5.0",
        "ifcastle/protocol-contracts": "^0.7",
        "league/uri": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.60",
        "phpunit/phpunit": "^11.3",
        "ifcastle/codestyle": "^0.1.5",
        "phpstan/phpstan": "^1.12",
        "symfony/console": "^7.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit ./tests",
        "analysis": "phpstan"
    }
}
